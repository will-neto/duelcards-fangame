@using DuelCards.FanGame.Domain.Entities
@model List<Card>


<table class="table table-hover">

    <tbody>
        @foreach (var card in Model)
        {
            <tr>
                <td>@card.Id</td>
                <td>@card.Name </td>
                <td>@card.Element</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@card.Id"> Editar </a>
                </td>
            </tr>
        }
    </tbody>
</table>